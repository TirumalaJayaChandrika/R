#data<-c(63,63,66,67,68,69,70,70,71,71)
print("enter data")
data<-scan()
print("enter given mue value")
u<-scan()
print("enter level of significance")
a<-scan()
n<-length(data)

te<-t.test(data,mu=u,alternative="two.sided")
print(te)
tv<-qt(1-a/2,df=n-1)
tv<-round(tv,digits=3)
cat("table value for right-tailed test :",tv,"\n")
tv1<-qt(1-a,df=n-1)
tv1<-round(tv1,digits=3)
cat("tble value for one -tailed test",tv1)
